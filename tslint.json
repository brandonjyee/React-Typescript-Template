{
	"extends": [
		"tslint:recommended",
		"tslint-react"
	],
	"rules": {
		"arrow-parens": false,
		"indent": [
			true,
			"tabs"
		],
		"interface-name": [
			false
		],
		"jsx-boolean-value": [
			true,
			"never"
		],
		"jsx-wrap-multiline": false,
		"object-literal-sort-keys": false,
		"ordered-imports": [
			false
		],
		"quotemark": [
			true,
			"single"
		],
		"no-require-imports": true
	}
}
